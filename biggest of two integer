#include <stdio.h>

int main() {
    int num1, num2;

    printf("Enter two integers (separated by spaces): ");
    scanf("%d %d", &num1, &num2);
    if (num1 > num2) {
        printf("%d is the biggest number.\n", num1);
    } else {
        printf("%d is the biggest number.\n", num2);
    }

    return 0;
}
